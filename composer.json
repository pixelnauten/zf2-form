{
    "name": "stroker/form",
    "description": "ZF2 module for extending forms with live clientside validation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "jquery validate",
        "form",
        "validation",
        "zend framework 2",
        "module"
    ],
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6 || >= 7.0",
        "laminas/laminas-eventmanager": "^2 || ^3",
        "laminas/laminas-form": "^2",
        "laminas/laminas-http": "^2",
        "laminas/laminas-json": "^2",
        "laminas/laminas-mvc": "^2",
        "laminas/laminas-cache": "^2",
        "laminas/laminas-config": "^2",
        "laminas/laminas-i18n": "^2",
        "laminas/laminas-i18n-resources": "^2",
        "laminas/laminas-inputfilter": "^2",
        "laminas/laminas-servicemanager": "^2",
        "laminas/laminas-stdlib": "^2 || ^3",
        "laminas/laminas-validator": "^2",
        "laminas/laminas-view": "^2"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "^1",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2"
    },
    "autoload": {
        "psr-0": {
            "StrokerForm": "src/",
            "StrokerFormTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
