{
    "name": "stroker/form",
    "description": "ZF2 module for extending forms with live clientside validation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "jquery validate",
        "form",
        "validation",
        "zend framework 2",
        "module"
    ],
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.0",
        "zendframework/zend-eventmanager": "^3.0.1",
        "zendframework/zend-form": "^2.9",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-config": "^3.0.0",
        "zendframework/zend-i18n": "^2.7.3",
        "zendframework/zend-i18n-resources": "^2.5.2",
        "zendframework/zend-inputfilter": "^2.7.2",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-view": "^2.8",
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/phpunit": "~5.5",
        "friendsofphp/php-cs-fixer": "~1.9"
    },
    "autoload": {
        "psr-0": {
            "StrokerForm": "src/",
            "StrokerFormTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
